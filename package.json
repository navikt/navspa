{
  "name": "@navikt/navspa",
  "version": "0.0.1",
  "description": "Bibliotek for eksportering og importering av hele applikasjoner som react-komponetner.",
  "repository": "github:navikt/navspa",
  "homepage": "https://github.com/navikt/navspa",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "commit": "git-cz",
    "start": "parcel example/index.html",
    "serve-cra-test": "serve -C example/cra-test",
    "serve-esm-test": "serve -C -l 2000 example/esm-test",
    "serve-react18-test": "serve -C -l 2001 example/react18-test"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "NAV",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.6.0 || 17 || 18",
    "react-dom": ">=16.6.0 || 17 || 18"
  },
  "devDependencies": {
    "@testing-library/react": "^12.1.0",
    "@types/jest": "^26.0.5",
    "@types/loadjs": "^4.0.0",
    "@types/node": "^12.6.8",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "git-cz": "^4.7.0",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^26.1.0",
    "parcel-bundler": "^1.12.4",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "serve": "^11.3.2",
    "ts-jest": "^26.1.3",
    "typescript": "^3.9.9",
    "yet-another-fetch-mock": "^4.1.0"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 65,
        "functions": 70,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "dependencies": {
    "loadjs": "^4.2.0"
  }
}
