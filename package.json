    "@types/loadjs": "^4.0.4",
    "@types/node": "^22.14.0",
    "@types/react": "^19.2.1",
    "@types/react-dom": "^19.2.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.1.1",
    "happy-dom": "^20.0.0",
    "isomorphic-fetch": "^3.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "serve": "^14.2.4",
    "typescript": "^5.8.3",
    "vite": "^6.2.5",
