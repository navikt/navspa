{
  "name": "@navikt/navspa",
  "version": "0.0.1",
  "description": "Bibliotek for eksportering og importering av hele applikasjoner som react-komponetner.",
  "repository": "github:navikt/navspa",
  "homepage": "https://github.com/navikt/navspa",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "commit": "git-cz"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "NAV",
  "license": "MIT",
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.8",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.5",
    "git-cz": "^3.2.1",
    "jest": "^24.8.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  }
}
